{
    "rewrites": [
        {
            "source": "/reformas(/?)(.*)",
            "destination": "https://reformasempresas.vercel.app/$2"
        }
    ],
    "headers": [
        {
            "source": "/reformas(/?)(.*)",
            "headers": [
                {
                    "key": "x-middleware-rewrite",
                    "value": "/$2"
                },
                {
                    "key": "x-vercel-base-path",
                    "value": "/reformas"
                }
            ]
        }
    ]
}