{
    "rewrites": [
        {
            "source": "/reformas/(.*)",
            "destination": "https://reformasempresas.vercel.app/$1"
        },
        {
            "source": "/reformas",
            "destination": "https://reformasempresas.vercel.app"
        }
    ],
    "headers": [
        {
            "source": "/reformas(/?)(.*)",
            "headers": [
                {
                    "key": "x-middleware-rewrite",
                    "value": "https://reformasempresas.vercel.app/$2"
                }
            ]
        }
    ]
}