{
    "rewrites": [
        {
            "source": "/reformas",
            "destination": "https://reformasempresas.vercel.app",
            "has": [
                {
                    "type": "header",
                    "key": "x-vercel-rewrite",
                    "value": "true"
                }
            ]
        },
        {
            "source": "/reformas/:path*",
            "destination": "https://reformasempresas.vercel.app/:path*"
        }
    ],
    "headers": [
        {
            "source": "/reformas",
            "headers": [
                {
                    "key": "x-vercel-rewrite",
                    "value": "true"
                }
            ]
        }
    ]
}